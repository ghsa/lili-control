{
    "name": "ghsa/lili-control",
    "description": "Lib to make dashboards at Laravel an easier task",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Andrade",
            "email": "contato@gustavoandrade.net"
        }
    ],
    "require": {
        "laravelcollective/html": "^6.1",
        "intervention/image": "^2.5",
        "spatie/laravel-medialibrary": "^8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "LiliControl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LiliControl\\tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiliControl\\LiliControlServiceProvider"
            ]
        }
    }
}
